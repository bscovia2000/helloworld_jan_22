pipeline {
    agent any
     stages{
      stages('Hello"){
       steps {
         echo "Hello World"
       }
    }
    
    stages('build"){
       steps {
         echo "build"
         sleep 5
       }
    }
    
    stages('test"){
       steps {
         echo "test"
         sleep 5
       }
    }
    
    stages('deploy"){
       steps {
         echo "deploy"
         sleep 5
       }
    }
    
    }

}
